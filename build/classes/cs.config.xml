<?xml version="1.0" encoding="UTF-8"?>
<config>

	<sendemail>1</sendemail>

	<api>
		<path>/csapi/rest/</path>
	</api>

	<jsp>
		<summary>/summary.jsp</summary>
		<form>/form.jsp</form>
		<details>/details.jsp</details>
	</jsp>

	<entities>
		<entity>lso</entity>
		<entity>gis</entity>
		<lso>
			<menuid>menu1</menuid>
			<domain>http://localhost:8080</domain>
			<main>/cs/json/lso.json</main>
			<image>/cs/images/lso.png</image>
			<title>LSO</title>
		</lso>
		<gis>
			<menuid>menu2</menuid>
			<domain>http://localhost:8080</domain>
			<link>/cs/json/lso.json</link>
			<image>/cs/images/gis.png</image>
			<title>GIS</title>
		</gis>
	</entities>


	<messsages>
		<cs200>SUCCESS</cs200>
		
		<cs400>PERMISSION DENIED NO USER ROLE BAD REQUEST</cs400>
		<cs401>UNAUTHORIZED</cs401>
		<cs402>SESSION EXPIRED</cs402>
		<cs403>INVALID TOKEN </cs403>
		<cs404>NOT FOUND</cs404>
		<cs406>NOT ACCEPTABLE SEND VIA HTTPS</cs406>
		<cs412>PRECONDITION FAILED</cs412>
		
		<cs500>EXCEPTION WHILE PROCESSING YOUR REQUEST INTERNAL SERVER ERROR </cs500>
	</messsages>

	<choiceslocation>C:/DL/csfiles/choices/</choiceslocation>	
	<print>
		<transaction>http://localhost:8080/csapi/print/transaction.html</transaction>
	</print>	
</config>







